background(255);
size(500, 500);

float x = 10;
while(x < width) {
  float y = 10;
  while(y < height) {
    noStroke();
    colorMode(HSB, 100);
    for (int i = 0; i < 100; i++) {
      for (int j = 0; j < 100; j++) {
        fill(i, j, 100);
        point(i, j);
      }
    ellipse(x, y, 15, 15);
    y = y + 20;
  }
  x = x + 20;
  }
}
  
