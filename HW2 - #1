background(0);
size(500, 500);

int x = 10;
int y = 10;

int x2 = 5;
int y2 = 5;

while (x < 500) {
  colorMode(HSB, 100);
  for (int i = 0; i < 100; i++) {
    for (int j = 0; j < 100; j++) {
      stroke(0);
      fill(i, j, 100);
      point(i, 100);
      ellipse (x, y, x2, y2);
      x = (x + 20);
      y = (y + 20);
      x2 = (x2 + 2);
      y2 = (y2 + 2);
    }
  }
}
